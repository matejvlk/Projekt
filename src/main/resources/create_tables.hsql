CREATE TABLE cities
(
    id INT GENERATED BY DEFAULT AS IDENTITY (START WITH 1, INCREMENT BY 1) NOT NULL,
    cityName VARCHAR(600) NOT NULL,
    stateName VARCHAR(60) NOT NULL,
    PRIMARY KEY (id, stateName)
);
CREATE TABLE states
(
    stateName VARCHAR(60) PRIMARY KEY NOT NULL,
    enabled TINYINT,
    authority VARCHAR(45) NOT NULL
);
ALTER TABLE cities ADD FOREIGN KEY (stateName) REFERENCES states (stateName);
CREATE INDEX fk_cities_states_idx ON cities (stateName);